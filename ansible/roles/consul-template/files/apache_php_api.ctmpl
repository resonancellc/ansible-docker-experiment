Listen 8081
<VirtualHost *:8081>
{{range services}}{{range service .Name}}{{if in .Tags "php_api"}}
    ProxyPassMatch ^{{.ServiceMeta.rule}}(.*)$ fcgi://127.0.0.1:{{.Port}}/var/www/html/public/index.php
{{end}}{{end}}{{end}}
    CustomLog /proc/self/fd/1 common
    ErrorLog /proc/self/fd/2
</VirtualHost>
